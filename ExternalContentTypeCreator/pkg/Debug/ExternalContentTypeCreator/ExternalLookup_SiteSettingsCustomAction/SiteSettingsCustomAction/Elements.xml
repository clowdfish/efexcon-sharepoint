<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <CustomActionGroup
     Id="EfexconConfigurationGroup"
     Location="Microsoft.SharePoint.SiteSettings"
     Title="$Resources:CustomAction_Title"
     Description="$Resources:CustomAction_Description"
     ImageUrl="/_layouts/Images/STSICON.GIF" />

  <CustomAction
    Id="DataSourceConfig"
    GroupId="EfexconConfigurationGroup"
    Location="Microsoft.SharePoint.SiteSettings"
    Description="$Resources:DataSource_Description"
    Title="$Resources:DataSource_Title"
    Rights="ManageWeb"
    ImageUrl="/_layouts/Images/STSICON.GIF">
    <UrlAction Url="~site/_layouts/EFEXCON/DataDefinition/DataSources.aspx"/>
  </CustomAction>

  <CustomAction
    Id="ExternalContentTypeConfig"
    GroupId="EfexconConfigurationGroup"
    Location="Microsoft.SharePoint.SiteSettings"
    Description="$Resources:ContentType_Description"
    Title="$Resources:ContentType_Title"
    Rights="ManageWeb"
    ImageUrl="/_layouts/Images/STSICON.GIF">
    <UrlAction Url="~site/_layouts/EFEXCON/DataDefinition/ExternalContentTypes.aspx"/>
  </CustomAction>
</Elements>
